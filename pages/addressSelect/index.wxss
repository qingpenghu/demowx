.selectAddress{
    font-size: 26rpx;
    line-height: 80rpx;
    border-bottom: 2rpx solid #f1f1f1;
}
.picker {
    font-size: 26rpx;
    line-height: 80rpx;
    border-bottom: 2rpx solid #f1f1f1;
}
.bottom{
    width: 100%;
    height: 120rpx;
    position: fixed;
    bottom: 0;
    left:0;
    display: flex;
    flex-wrap: nowrap;
    box-sizing: border-box;
    border-top: 2rpx solid #f1f1f1;
}
.cancel {
    flex:1;
    text-align: center;
    line-height: 120rpx;
    border-right: 2rpx solid #f1f1f1;
}
.confirm{
    flex: 1;
    text-align: center;
    line-height: 120rpx;
}

